<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multi.camp.camping">
	<select id="selectall" resultType="campingList">
		select * from camping_list
	</select>
	<select id="search" resultType="campingList" parameterType="String">
		select * from camping_list where camp_name like '%' || #{camp_name} ||'%'
	</select>
	<select id="search2" resultType="campingList" parameterType="Map">
		select * from camping_List where camp_name like #{search} and business_no = ${local}
	</select>
</mapper>